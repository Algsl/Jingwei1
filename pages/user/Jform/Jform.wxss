/*整体布局*
.jform{
  width: 100%;
  background-color:#EDF0EF;
}

/*头部布局*
.top{
  width: 100%;
  height:164rpx;
  display: flex;
  flex-direction: row;
  background-color: white;
}
/*头部图片*
.topimg{
  width:132rpx ;
  height:132rpx;
  border-radius:50%;
  margin: 15rpx;
  background-color:grey;
}
/*头部右侧*
.topright{
  display: flex;
  flex-direction: column;
  margin-top: 15rpx;
  margin-left: 30rpx;
  width: 70%;
  height: 132rpx;
}
/*头部右上*
.toprightup{
  width: 100%;
  height: 70rpx;
  line-height: 70rpx;
  font-size: 32rpx;
