.toprightdown{
  width: 100%;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 24rpx;
  color:red;
}

/*内容布局*
.content{
  margin-top: 20rpx;
  width: 100%;
  background-color:white;
}
/*内容条目*
.content_item{
  display: flex;
  flex-direction: row;
  width: 100%;
  height: 96rpx;
  font-size: 28rpx;
  border-bottom: 1rpx solid #D6D6D6; 
}
/*内容条目左侧*
.content_item_left{
  display: flex;
  width: 200rpx;
  height: 60rpx;
  line-height: 60rpx;
  margin-left: 15rpx;
  margin-top: 18rpx;
}
/*内容条目右侧*
.content_item_right{
  width: 50%;
  height: 60rpx;
  line-height: 60rpx;
  margin-left: 30rpx;
  margin-top: 18rpx;
}
.content_item_right1{
  width: 40%;
