.main{
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  background:  #F7F9FC; 
}
.content{
  width: 100%;
  height: 60px;
  display: flex;
  flex-direction: row;
}
.content_view{
  width: 70%;
  height: 30px;
  margin:15px 20px;
  display: flex;
  flex-direction: row;
  font-size: 14px;
  color: #2B375E;
  border-radius: 4px;
  background: #FFFFFF;
}
.content_view_img{
  width: 20px;
  height: 20px;
  margin: 5px;
}
.content_view_input{
  width: 200px;
  height: 30px;
  line-height: 30px;
}
.content_view_btn{
  width: 60px;
  height: 30px;
  line-height: 30px;
  text-align: center;
  margin-top: 15px;
  font-size: 14px;
  color: #FFFFFF;
  background: #1EC996;
}
.content3{
  width: 100%;
  height: 100%;
  background: #FFFFFF;
}
.content3_item{
  width: 100%;
  height: 103px;
  display: flex;
  flex-direction: column;
  border-bottom: 0.5px solid #E1E2E6;
}
.split{
  width: 12px;
  height: 16px;
  line-height: 16px;
  text-align: center;
  font-size: 12px;
  color: #E1E2E6;
}
.split1{
  width: 12px;
  height: 22px;
  line-height: 22px;
  text-align: center;
  font-size: 12px;
  color: #E1E2E6;
}
.content3_item_1{
  width: 100%;
  height: 22px;
  display: flex;
  flex-direction: row;
  margin-left: 5.3%;
  margin-top: 18px;
}
.content3_item_1_txt1{
  width: 238px;
  height: 22px;
  line-height: 22px;
  font-size: 16px;
  color: #202437;
}
.content3_item_1_txt2{
  height: 22px;
  margin-left: 3.2%;
  font-size: 18px;
  color: #1EC996;
}
.content3_item_1_txt3{
  height: 22px;
  line-height: 22px;
  font-size: 12px;
  color: #797E8B;
}
.content3_item_2{
  width: 100%;
  height: 16px;
  line-height: 16px;
  margin-top: 3px;
  display: flex;
  flex-direction: row;
}
.content3_item_2_img{
  width: 14px;
  height: 14px;
  margin-left: 5.3%;
  margin-top: 1px;
}
.content3_item_2_txt{
  width: 100%;
  height: 16px;
  line-height: 16px;
  margin-left: 1.1%;
  font-size: 12px;
  color: #A0A3AF;
}
.content3_item_3{
  width: 100%;
  height: 16px;
  display: flex;
  flex-direction: row;
  line-height: 16px;
  margin-left: 5.3%;
  margin-top: 10px;
}
.content3_item_3_txt1{
  height: 16px;
  line-height: 16px;
  font-size: 14px;
  color: #515666;
}
.content3_item_3_txt2{
  font-size: 14px;
  color: #515666;
}
.content3_item_3_txt3{
  font-size: 14px;
  color: #797E8B;
}
.content3_item_3_txt4{
  margin-left: 7.1%;
  font-size: 14px;
  color: #A0A3AF;
}
.over{
  width: 100%;
  height: 50px;
  line-height: 50px;
  text-align: center;
  font-size: 16px;
  color: #202437;
  background: #FFFFFF;
}